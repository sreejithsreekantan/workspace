package week3

object Rational {
  println("Welcome to the Scala worksheet")       //> Welcome to the Scala worksheet
	new Rational(1, 2)                        //> res0: week3.Rational = week3.Rational@ccec5f
	
	val n = new Rational(1, 2)                //> n  : week3.Rational = week3.Rational@1b289cd
	n.numer                                   //> res1: Int = 1
	n.denom                                   //> res2: Int = 2
}

class Rational(x: Int, y: Int) {
	def numer = x
	def denom = y
}